{
    "name": "anpoltanov/sane_php_frontend",
    "description": "Web UI for SANE. Uses scanimage.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andrey Poltanov",
            "email": "anpoltanov@yandex.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "symfony/flex": "2.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/yaml": "5.4.*",
        "symfony/console": "5.4.*",
        "sensio/framework-extra-bundle": "6.1.*",
        "symfony/twig-bundle": "5.4.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "symfony/form": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/asset": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.16"
    },
    "require-dev": {
        "symfony/web-profiler-bundle": "5.4.*",
        "symfony/debug-bundle": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony": {
            "require": "5.4.*"
        }
    }
}
