{
    "name": "anpoltanov/sane_php_frontend",
    "description": "Web UI for SANE. Uses scanimage.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Andrey Poltanov",
            "email": "anpoltanov@yandex.com"
        }
    ],
    "require": {
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "^5.2",
        "symfony/dotenv": "^5.2",
        "symfony/yaml": "^5.2",
        "symfony/console": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "symfony/twig-bundle": "^5.2",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/form": "^5.2",
        "symfony/validator": "^5.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.24"
        }
    }
}
